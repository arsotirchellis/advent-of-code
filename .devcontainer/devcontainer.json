// https://containers.dev/implementors/json_schema/
{
	"name": "Docker Compose",
	"dockerComposeFile": [
		"./docker-compose.yml"
	],
	"service": "advent-of-code-2023-dev-container",
	"workspaceFolder": "/advent-of-code-2023",
	"customizations": {
		"vscode": {
			"extensions": [
				"github.vscode-github-actions",
				"GitHub.copilot",
				"qezhu.gitlink",
				"streetsidesoftware.code-spell-checker",
				"ms-vsliveshare.vsliveshare",
				"eamodio.gitlens",
				"ms-azuretools.vscode-docker",
				"IronGeek.vscode-env",
				"ms-python.python",
				"zxh404.vscode-proto3",
				"mechatroner.rainbow-csv",
				"janisdd.vscode-edit-csv",
				"Alpha4.jsonl",
				"GitHub.copilot-chat",
				"ms-python.debugpy",
				"ms-python.vscode-pylance",
				"ms-python.autopep8"
			]
		}
	},
	"postStartCommand": "poetry install"
}